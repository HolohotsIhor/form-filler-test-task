@font-face {
    font-family 'Open Sans'
    font-display swap
    src url("/../fonts/OpenSans-Regular.woff") format('woff'), url("/../fonts/OpenSans-Regular.woff2") format('woff2'), url("/src/fonts/OpenSans-Regular.ttf") format('ttf')
    font-weight 400
    font-style normal
}

